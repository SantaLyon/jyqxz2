config_version=4

[application]

config/name="金庸群侠传2复刻"
config/description="从小就喜欢玩半瓶神仙醋的金庸群侠传，最近正在学习Godot引擎，借此机会对童年最喜欢的游戏进行一波复刻。"
run/main_scene="res://src/GameStart.tscn"
config/use_custom_user_dir=true
config/icon="res://asset/image/ui/Image 8508.png"

[autoload]

SceneChanger="*res://src/autoload/SceneChanger.tscn"
Dialog="*res://src/autoload/Dialog.tscn"
GameEvent="*res://src/autoload/GameEvent.tscn"
GameStatus="*res://src/autoload/GameStatus.gd"

[display]

window/size/width=960
window/size/height=800
window/size/test_width=960
window/size/test_height=800
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

